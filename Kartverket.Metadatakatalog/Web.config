  <!-- For more information on Entity Framework configuration, visit http://go.microsoft.com/fwlink/?LinkID=237468 --></configSections>
  <basicAuth configSource="authentication.config" />
  <connectionStrings>
    <add name="DefaultConnection" connectionString="Server=.\SQLEXPRESS;Database=kartverket_kartkatalog;Integrated Security=True;" providerName="System.Data.SqlClient" />
  </connectionStrings>
  <log4net debug="true">
    <root>
      <priority value="DEBUG" />
      <appender-ref ref="TraceAppender" />
      <appender-ref ref="LogFileAppender" />
    </root>
    <appender name="LogFileAppender" type="log4net.Appender.RollingFileAppender">
      <file value="c:\\inetpub\\logs\\Metadatakatalog" />
      <appendToFile value="true" />
      <rollingStyle value="Date" />
      <param name="StaticLogFileName" value="false" />
      <datePattern value=".yyyyMMdd.lo\g" />
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%date [%thread] %-5level %logger - %message%newline" />
      </layout>
    </appender>
    <appender name="TraceAppender" type="log4net.Appender.TraceAppender">
      <layout type="log4net.Layout.PatternLayout">
        <conversionPattern value="%-6level - %message%newline" />
      </layout>
    </appender>
    <logger name="GeoNorgeAPI.HttpRequestExecutor">
      <level value="INFO" />
    </logger>
  </log4net>
  <appSettings file="settings.config">
    <add key="MaxRecursionLimit" value="10000" />
