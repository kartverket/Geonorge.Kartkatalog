    <add key="GeonorgeWmsUrl" value="https://wms.geonorge.no/" />
    <add key="ObjektkatalogUrl" value="https://objektkatalog.dev.geonorge.no/" />
    <add key="KartkatalogenUrl" value="http://localhost:50081/" />
    <add key="DownloadUrl" value="https://localhost:44350/" />
    <add key="StatusApiUrl" value="https://status.geonorge.no/test" />
    <add key="WebmasterEmail" value="post@norgedigitalt.no" />
    <add key="SupportsMultiCulture" value="true" />
    <add key="BuildVersionNumber" value="1.0" />
    <add key="webpages:Version" value="3.0.0.0" />
    <add key="webpages:Enabled" value="false" />
    <add key="ClientValidationEnabled" value="true" />
    <add key="UnobtrusiveJavaScriptEnabled" value="true" />
    <add key="DownloadServiceEnabled" value="true" />
    <add key="SearchOptionsFile" value="searchOption.local.js" />
    <add key="EnvironmentName" value="dev" />
    <add key="WhitespaceFilterEnabled" value="true" />
    <add key="CompressionFilterEnabled" value="true" />
